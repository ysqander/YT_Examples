#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/openai@4.75.0_zod@3.23.8/node_modules/openai/bin/node_modules:/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/openai@4.75.0_zod@3.23.8/node_modules/openai/node_modules:/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/openai@4.75.0_zod@3.23.8/node_modules:/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/openai@4.75.0_zod@3.23.8/node_modules/openai/bin/node_modules:/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/openai@4.75.0_zod@3.23.8/node_modules/openai/node_modules:/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/openai@4.75.0_zod@3.23.8/node_modules:/Users/alexanderadamov/Documents/Programming/YT_Examples/evals_and_disillation/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli" "$@"
else
  exec node  "$basedir/../../bin/cli" "$@"
fi
